{% extends '::base.html.twig' %}

{% block head %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->
{% endblock %}

{% block stylesheets %}
    {% stylesheets  'bundles/redsolidariamain/bootstrap-3.0.3/css/bootstrap.min.css' 
                    'bundles/redsolidariamain/bootstrap-3.0.3/css/navbar.css' 
                    filter='cssrewrite' 
    %}
        <!-- Bootstrap -->
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}

    {% stylesheets 'bundles/redsolidariamain/jquery-ui-1.10.3/themes/base/jquery-ui.css' filter='cssrewrite' %}
        <!-- jQuery UI -->
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}
        
{% block javascripts %}
    {% javascripts  '@RedSolidariaMainBundle/Resources/public/jquery-ui-1.10.3/jquery-1.9.1.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts '@RedSolidariaMainBundle/Resources/public/jquery-ui-1.10.3/ui/jquery-ui.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}

    {% javascripts '@RedSolidariaMainBundle/Resources/public/bootstrap-3.0.3/js/bootstrap.min.js' %}
        <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}